{
  "source": {
    "include": ["./src"],
    "exclude": ["./node_modules"]
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "recurse": true,
    "template": "node_modules/docdash",
    "verbose": true,
    "tutorial": "./tutorials"
  },
  "plugins": ["node_modules/jsdoc-babel"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "outputSourceFiles": false,
    "systemName": "DynSJS doc"

  }
}
