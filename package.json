{
    "name": "DynSJS",
    "version": "1.0.0",
    "description": "Une bibliothèque JS pour générer dynamiquement des feuilles de style CSS.",
    "main": "index.js",
    "scripts": {
        "test": "jest tests/DynSJS.test.js",
        "build": "npm run clean && webpack --config webpack.config.cjs",
        "build:dev:auto": "nodemon --exec npm run build:dev",
        "build:dev": "node compileStyles.js",
        "build:dev:clean": "rm -rf dist/build/*",
        "build:dev:minify": "terser-webpack-plugin dist/build/index.js dist/build/index.min.js",
        "new:module": "node generateModule.js",
        "doc": "jsdoc -c jsdoc.json"
    },
    "keywords": [
        "css",
        "stylesheet",
        "dynamic",
        "js"
    ],
    "author": "Paterne Guélablé Gnonzion",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.22.10",
        "@babel/preset-env": "^7.22.10",
        "babel-loader": "^9.1.3",
        "better-docs": "^2.7.2",
        "chokidar-cli": "^3.0.0",
        "docdash": "^2.0.1",
        "jest": "^29.6.4",
        "jsdoc": "^4.0.2",
        "jsdoc-babel": "^0.5.0",
        "jsdoc-webpack-plugin": "^0.3.0",
        "minami": "^1.2.3",
        "nodemon": "^3.0.1",
        "terser-webpack-plugin": "^5.3.9",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4"
    },
    "jest": {
        "testEnvironment": "node",
        "transform": {
            "^.+\\.js$": "babel-jest"
        }
    },
    "type": "module",
    "dependencies": {
        "canvas": "^2.11.2",
        "expect": "^29.6.4",
        "jsdom": "^22.1.0",
        "tui-jsdoc-template": "^1.2.2"
    }
}
